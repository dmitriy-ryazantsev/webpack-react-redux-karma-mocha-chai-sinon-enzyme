  },
  "engines": {
    "node": ">=4.0.0",
    "iojs": ">=1.1.0"
  },
  "license": "ISC",
  "scripts": {
    "debug": "webpack-dev-server --config webpack.config.debug.js --progress --inline --hot",
    "build": "webpack --config webpack.config.build.js --progress --colors",
    "build:release": "webpack --colors",
    "Release": "npm run test:release && npm run build:release",
    "test": "webpack-dev-server --config webpack.config.test.js --hot --inline",
    "test:karma": "karma start --auto-watch=true --single-run=false",
    "test:release": "karma start"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
